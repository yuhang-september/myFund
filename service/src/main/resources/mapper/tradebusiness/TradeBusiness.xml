<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yuhang.service.mapper.tradebusiness.TradeBusinessMapper">

    <sql id="allColumns">
        trade_business_id,
        trade_request_id,
        fund_code,
        amount,
        share,
        create_time,
        last_update_time,
        status,
        type
    </sql>

    <resultMap id="Business" type="com.yuhang.service.entity.request.TradeBusiness">
        <id column="trade_business_id" property="businessId" javaType="String" jdbcType="CHAR"/>
        <result column="trade_request_id" property="tradeRequestId" javaType="String" jdbcType="CHAR"/>
        <result column="fund_code" property="fundCode" javaType="String" jdbcType="VARCHAR"/>
        <result column="amount" property="amount" javaType="Double" jdbcType="DECIMAL"/>
        <result column="status" property="status" javaType="Integer" jdbcType="INTEGER"/>
        <result column="type" property="type" javaType="String" jdbcType="VARCHAR"/>
        <result column="create_time" property="createTime" javaType="Date" jdbcType="TIMESTAMP"/>
        <result column="last_update_time" property="lastUpdateTime" javaType="Date" jdbcType="TIMESTAMP"/>
    </resultMap>

    <insert id="insert" parameterType="com.yuhang.service.entity.request.TradeBusiness">
        insert into tr_trade_business (<include refid="allColumns"/>) value (
        #{business.businessId},#{business.tradeRequestId},#{business.fundCode},#{business.amount},#{business.share},
        #{business.createTime},#{business.lastUpdateTime},#{business.status},#{business.type}
        )
    </insert>
</mapper>