<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yuhang.service.mapper.system.DictionaryItemMapper">

    <resultMap id="dictionaryItem" type="com.yuhang.service.entity.system.DictionaryItem">
        <id column="id" property="id" javaType="String" jdbcType="CHAR"/>
        <result column="name" property="name" javaType="String" jdbcType="VARCHAR"/>
        <result column="value" property="value" javaType="String" jdbcType="CHAR"/>
        <result column="dictionary_id" property="dictionaryId" javaType="String" jdbcType="CHAR"/>
    </resultMap>

    <insert id="insert" parameterType="com.yuhang.service.entity.system.DictionaryItem">
        insert into sys_dictionary_item (id, name, value, dictionary_id) value (
             #{dictionaryItem.id}, #{dictionaryItem.name}, #{dictionaryItem.value}, #{dictionaryItem.dictionaryId}
            )
    </insert>

    <select id="selectById" resultMap="dictionaryItem">
        SELECT id, name, value, dictionary_id
        FROM sys_dictionary_item
        where id = #{id}
    </select>

    <select id="selectAll" resultMap="dictionaryItem">
        select id, name, value, dictionary_id
        from sys_dictionary_item
    </select>
</mapper>